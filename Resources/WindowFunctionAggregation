        "%%sql\n",
        "WITH orders_2022 AS(\n",
        "  SELECT DISTINCT\n",
        "    customerkey,\n",
        "    orderkey,\n",
        "    orderdate\n",
        "  FROM sales\n",
        "  WHERE EXTRACT(YEAR FROM orderdate) = 2022\n",
        ")\n",
        "SELECT *,\n",
        "  COUNT(*) OVER(PARTITION BY customerkey) AS orders_per_customer\n",
        "FROM orders_2022\n",
        "ORDER BY orders_per_customer DESC, customerkey\n"
      ],
